{
  "name": "eternalrival-react2023q4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint:cmd": "eslint --ext .ts,.tsx --report-unused-disable-directives --no-inline-config --max-warnings 0",
    "lint": "npm run lint:cmd -- .",
    "lint:fix": "npm run lint -- --fix",
    "lints:fix": "stylelint --mw 0 --fix \"src/**/*.css\"",
    "fix": "prettier -luw .",
    "fix:all": "npm run fix && npm run lint:fix && npm run lints:fix",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier -luw",
    "*.{ts,tsx}": "npm run lint:cmd -- --fix",
    "*.css": "stylelint --mw 0 --fix"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-clean-order"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "selector-class-pattern": null,
      "selector-id-pattern": null
    }
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80,
    "arrowParens": "always"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "20.10.3",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.55.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "husky": "8.0.3",
    "jsdom": "23.0.1",
    "lint-staged": "15.2.0",
    "prettier": "3.1.0",
    "sass": "1.69.5",
    "stylelint": "15.11.0",
    "stylelint-config-clean-order": "5.2.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
    "@reduxjs/toolkit": "1.9.7",
    "csstype": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-redux": "8.1.3",
    "react-router-dom": "6.20.0",
    "uuid": "9.0.1",
    "yup": "1.3.2"
  }
}
