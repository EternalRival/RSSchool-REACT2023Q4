{
  "name": "eternalrival-react2023q4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:cmd": "eslint --ext .ts,.tsx --report-unused-disable-directives --no-inline-config --max-warnings 0",
    "lint": "npm run lint:cmd -- .",
    "lint:fix": "npm run lint -- --fix",
    "lints:fix": "stylelint --mw 0 --fix \"src/**/*.css\"",
    "fix": "prettier -luw .",
    "fix:all": "npm run fix && npm run lint:fix && npm run lints:fix",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier -luw",
    "*.{ts,tsx}": "npm run lint:cmd -- --fix",
    "*.css": "stylelint --mw 0 --fix"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-clean-order"
    ],
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]+$"
    }
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80,
    "arrowParens": "always"
  },
  "devDependencies": {
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react": "4.2.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.4",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "stylelint": "15.11.0",
    "stylelint-config-clean-order": "5.2.0",
    "stylelint-config-standard": "34.0.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2",
    "vite": "5.0.3"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-redux": "8.1.3",
    "react-router-dom": "6.20.0",
    "yup": "1.3.2"
  }
}
